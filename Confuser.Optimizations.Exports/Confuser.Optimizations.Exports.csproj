    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\ConfuserEx.snk</AssemblyOriginatorKeyFile>
    <EnableCodeAnalysis>true</EnableCodeAnalysis>
  </PropertyGroup>

  <PropertyGroup Label="Assembly Information">
